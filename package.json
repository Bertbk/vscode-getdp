{
	"author": "Bertrand Thierry <bthierry@math.cnrs.fr>",
	"bugs": {
		"url": "https://github.com/Bertbk/vscode-getdp/issues/"
	},
	"name": "vscode-getdp",
	"displayName": "vscode-getdp",
	"description": "GetDP language support in Visual Studio Code",
	"version": "0.0.2",
	"keywords": [
		"getdp",
		"language"
	],
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "getdp",
				"aliases": [
					"GetDP",
					"getdp"
				],
				"extensions": [
					".pro"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "getdp",
				"scopeName": "source.pro",
				"path": "./syntaxes/getdp.tmLanguage.json"
			}
		]
	},
	"homepage": "https://getdp.info/",
	"license": "MIT",
	"publisher": "Bertrand-Thierry",
	"repository": {
		"type": "git",
		"url": "https://github.com/Bertbk/vscode-getdp.git"
	}
}