{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"scopeName": "source.pro",
	"patterns": [
		{
			"include": "#comments"
		},
		{
			"include": "#punctuation"
		},
		{
			"include": "#operators"
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#constants"
		}
	],
	"repository": {
		"comments": {
			"comment": "// comment",
			"patterns": [
				{
					"name": "comment.line.getdp",
					"match": "//.*"
				},
				{
					"name": "comment.block.getdp",
					"begin": "/\\*",
					"end": "\\*/",
					"patterns": [
						{}
					]
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.getdp",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.getdp",
					"match": "\\\\."
				}
			]
		},
		"operators": {
			"patterns": [
				{
					"name": "keyword.operator.getdp",
					"match": "(\\+|-|\\*|/|>|<|=|<=|>=|==|%|\\+=|-=|/=|\\*=|\\+\\+|--|\\^|!=)"
				},
				{
					"name": "keyword.operator.logical.getdp",
					"match": "(!|&&|\\|\\||\\?)"
				}
			]
		},
		"punctuation": {
			"patterns": [
				{
					"name": "punctuation.terminator.getdp",
					"match": ";"
				},
				{
					"name": "punctuation.separator.getdp",
					"match": "(,|:)"
				},
				{
					"name": "punctuation.accessor.getdp",
					"match": "(\\.)"
				}
			]
		},
		"constants": {
			"patterns": [
				{
					"name": "constant.language.getdp",
					"match": "Pi"
				}
			]
		},
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.getdp",
					"match": "\\b(Include|For|If|Else|While|In|EndFor|EndWhile|EndIf|Else|ElseIf)\\b"
				},
				{
					"name": " entity.name.class.getdp",
					"match": "\\b(Group|Constraint|Jacobian|Integration|Function|FunctionSpace|Formulation|Resolution|PostProcessing|PostOperation)\\b"
				},
				{
					"name": "keyword.getdp",
					"match": "\\b(Choices|ReadOnly|Help|Visible)\\b"
				},
				{
					"name": "entity.name.function.getdp",
					"match": "\\b(GetRegions|StrCat|StrFind|StrCmp|StrLen|StringToName|S2N|Exists|FileExists|GroupExists|GetForced|NbrRegions|GetNumber|Print|Printf)\\b"
				},
				{
					"name": "keyword.getdp",
					"match": "\\b(InitMovingBand2D|MeshMovingBand2D|MovingBand2D|Coefficient|RegionRef|SubRegionRef|Exit|Galerkin|Frequency|ENABLE_NX|CreateSolution|InitFromResolution|BF_GradNode|Generate|Units|InterpolationBilinear|TensorSym|AppendTimeStepToFileName|Local|AppendExpressionFormat|SetDTime|IterativeLoop|ENABLE_ZITSOL|UnitVectorX|ENABLE_PETSC|ChangeOfCoordinates|Else|Gmsh|ENABLE_HPDDM|Name|Group|TTrace|NameOfCoef|Lanczos|GlobalEquation|QuadraturePointIndex|TransferInitSolution|DualEdgesOf|Integration|Link|OnElementsOf|Form0|CompElementNum|ChangeOfValues|ENABLE_BUILD_DYNAMIC|System|VolAxiRectShell|GlobalTerm|ENABLE_MMA|VolAxiSqu|Node|ComplexScalarField|$Theta|$Iteration|VolCylShell|ENABLE_BLAS_LAPACK|GmshParsed|BF_NodeX|Field|SetTimeStep|3D|Cart2Cyl|Norm|AppendExpressionToFileName|SaveSolution|DefineGroup|ElementTable|NameOfBasisFunction|BF_RegionX|BF_RegionZ|Table|Scalar|LinkCplx|StoreMinYinRegister|LastTimeStepOnly|Abs|HarmonicToTime|AssociatedWith|NameOfSpace|VolAxiSquRectShell|ENABLE_BUILD_IOS|dFunction|Operation|deRham|ElementsOf|BF_GroupOfNodes|GradHelmholtz|In|Tensor|Min|$EigenvalueReal|InitSolution1|Function|dInterpolationLinear|Grad|NormalSource|OnPlane|SetRightHandSideAsSolution|ENABLE_NR|ElementVol|Quantity|GetResidual|OnSection|BF_RegionY|dYn|Sign|Tan|PostProcessing|BF_CurlGroupOfEdges|Prism|EndFor|GetWallClockTime|Asin|CompXY|DeleteFile|BF_CurlGroupOfPerpendicularEdge|Yn|SurfaceArea|Parse|TensorField|Hypot|Return|Rotate|StoreMaxZinRegister|Rot|Cart2Pol|Cart2Sph|Div|ENABLE_PYTHON|InterpolationAkima|Exp|StoreMinZinRegister|dInterpolationBilinear|CreateDir|CreateDirectory|TimeStep|NameOfConstraint|DualNodesOf|StoreMaxYinRegister|CompYX|IterativeLoopN|Order|Hidden|TangentSource|Complex_MH|BF_GroupOfEdges|GroupsOfEdgesOf|Dof|Equation|VolAxiSquSphShell|Resolution|VolumesOf|SwapSolutionAndResidual|EigenSolve|FacetsOfTreeIn|Curl|RenameFile|VolAxi|Target|Transpose|StoreInVariable|BF_PerpendicularEdge|GroupOfRegionsOf|GeoElement|GetCpuTime|ElseIf|Interval|Sur|Norma